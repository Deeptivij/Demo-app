.actions{
  border-color: #8fc800;
  background: #9400D3;
  background: -moz-linear-gradient(top, #9400D3 0%, #9400D3 100%);
  background: -webkit-gradient(linear, left top, left bottom, 
    color-stop(0%,#8fc800), color-stop(100%,#438c00));
  background: -webkit-linear-gradient(top, #8fc800 0%,#438c00 100%);
  background: -o-linear-gradient(top, #8fc800 0%,#438c00 100%);
  background: -ms-linear-gradient(top, #8fc800 0%,#438c00 100%);
  background: linear-gradient(top, #8fc800 0%,#438c00 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( 
    startColorstr='#8fc800', endColorstr='#438c00',GradientType=0 );
}
.field {
color: #FF1493;
font: 100 30px/1.5 'Pacifico', Helvetica, sans-serif;
}
.chart {
    border-radius: 25px;
    border: 6px solid #8AC007;
    padding: 20px;
    width: 300px;
    height: 450px;
}

#top, #bottom, #left, #right {
	background: #FF1493;
	position: fixed;
	}
	#left, #right {
		top: 0; bottom: 0;
		width: 4px;
		}
		#left { left: 0; }
		#right { right: 0; }
		
	#top, #bottom {
		left: 0; right: 0;
		height: 4px;
		}
		#top { top: 0; }
		#bottom { bottom: 0; }
		@import url(http://fonts.googleapis.com/css?family=Concert+One);
}
bod {
  background : #1b1b1b;
}
div:after , div:before {
  display : block;
  content : "";
  position: absolute;
  z-index : -1;
}

.bod {
  position : relative; 
  margin   : 100px auto;
  width    : 30px;
  height   : 150px;
  border-radius : 50%;
  background : #0ff;
  border     : 1px solid #eee;
}

.bod:before , .bod:after {
  top    : -80px;
  left   : 0;
  width  : 1px;
  height : 60px;
  background : #f0f;
  transform  : rotate(-20deg);
}

.bod:after {
  left  : 30px;
  transform  : rotate(20deg);	
}

.head {
  position : absolute;
  bottom   : 99%;
  left     : -5px;
  width    : 40px;
  height   : 40px;
  border-radius : 50%;
  background : #0ff;
  box-shadow : inset 0 0 10px 0 #f4f;
}		

.head:before , .head:after {
  top    : -45px;
  left   : -10px;
  width  : 10px;
  height : 10px;
  border-radius : 50%;
  background : #f0f;
}

.head:after {
  left : 40px;
}


.left  , .right{
  width  : 50px;
  height : 50px;
  background : #f0f;
  position   : absolute;
  top        : 50px;
  z-index    : -1;
  perspective : 250;
}

.left {
  right : 100%;
  transform-origin : 100% 0;
  animation : l-fly .7s ease-out infinite alternate both;
}

.right  {
  left : 100%;
  transform-origin : 0 0;
  animation : fly .7s ease-out infinite alternate both;
}

.left:before , .right:before {
  width  : 130px;
  height : 200px;
  top    : -140px;
  z-index    : -2;
  background : #0ff;
  border-radius : 50%/60% 60% 40% 40%;
}

.left:before {
  right  : 0;
  transform : rotate(150deg);
  box-shadow : inset -5px -5px 5px 1px magenta;
}

.right:before {
  left   : 0;
  transform : rotate(-150deg);
  box-shadow : inset 5px -5px 5px 1px magenta;
}

.left:after , .right:after {
  width  : 100px;
  height : 160px;
  top    : -20px;
  z-index    : -2;
  background : #0ff;
  border-radius         : 50%/60% 60% 40% 40%;
}

.left:after {
  right  : 20px;
  transform : rotate(70deg);
  box-shadow : inset -1px -5px 5px 1px magenta;
}


.right:after {
  left   : 20px;
  transform : rotate(-70deg);
  box-shadow : inset 1px -5px 5px 1px magenta;
}

.front {
  position : absolute;
  top      : -90px;
  left     : -50px;
  z-index  : 2;
  width  : 50px;
  height : 50px;
  border-radius : 50%;
  background : #f0f;
  box-shadow : 0 0 10px 10px #ff0 , 0 120px 0 -10px #fb0 , 0 120px 10px 0 #fff;
}

.right .front {
  left     : 50px;
}

@keyframes fly {
  100% {
    transform  : rotateY(60deg);
  }
}

@keyframes l-fly {
  100% {
    transform  : rotateY(60deg);
  }
}
		
</style>
