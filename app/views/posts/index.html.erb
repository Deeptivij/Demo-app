<h1 id='ij'>Listing Albums</h1>
<%= link_to 'New album', new_post_path ,:id=>'kk'%>
<div style="float:right;width:300px;>
<div id="container">
  <div id="butterfly">
    <div class="left wingg"></div>
    <div class="right wingg"></div>
    
  </div>
</div>
</div>
<div class='my'>
<body>

    
 
  <% @user.posts.each do |post| %>
  
   <div style="border-width: 4px;border-height: 10px; border-style: ridge; border-color: red; width: 100px;
    height: 80px;margin-top: 90px;" id='gf'> 
    <tr>
    	
    	<div id="hf">
      <b>Title-</b> <td><%= post.title%></td><br>
      <b>Description-</b> <td><%= post.description %></td><br>
     <div class="flower violet">
<div class="leaf tl"></div>
<div class="leaf tr"></div>
<div class="leaf bl"></div>
<div class="leaf br"></div>
</div>
       </tr>
       <table>
       	<tr>
     <th> <%= button_to 'Show',post_path(post),method: :get,:id=>'df'%></th>
      <th><%= button_to 'Edit', edit_post_path(post),method: :get,:id=>'df1' %></th>
    <th>  <%= button_to 'Delete', post, method: :delete,:id=>'df2',       
              data: { confirm: 'Are you sure?' }%></th>
   </tr>
   </table>
    
    </div>
    </div>
  <% end %>
</table>
</div>
<script>
$(document).ready(function(){
	
    $(".new").click(function(e){
    	e.preventDefault();
    
       var s=$(this).attr('href');
       alert(s);
          $.ajax({
          	
       	
       method: "GET",
     
      
       url: s,
       
       success: function(d) {
     
       	
       
 $(".my").html(d);
 
 
     
       
      
   }
    })
    
});
});
	</script>
</body>

