}

.bl {
	border-top-right-radius: 0px;
	clear: left;
}

.br {
	border-top-left-radius: 0px;
}

.flower {
	float: left;
}

.flower:hover {
	-webkit-animation: bubbly 1s ease-in;
	-moz-animation: bubbly 1s ease-in;
	-ms-animation: bubbly 1s ease-in;
	-o-animation: bubbly 1s ease-in;
	animation: bubbly 1s ease-in;
}

